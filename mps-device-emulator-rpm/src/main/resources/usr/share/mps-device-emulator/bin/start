init() {
    EMULATOR_DATA="/var/log/mps-device-emulator"
}

run() {
    # Ensure the log directory exists 
    if [ ! -d "$EMULATOR_DATA" ]; then
        mkdir -p "$EMULATOR_DATA"
    fi
    exec java -Dserver.port=8091 -jar /usr/share/mps-device-emulator/mps-device-emulator-${mps.emulator.version}-jar-with-dependencies.jar &
}

main() {
    init
    run "$@"
}

main "$@"

