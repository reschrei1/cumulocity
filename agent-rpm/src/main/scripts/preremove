# stop karaf service
service cumulocity-demo-karaf stop
sleep 5

# modify org.apache.karaf.features.cfg file (remove bundle information)

sed -i '/cumulocity-mps-agent,/d' /usr/share/cumulocity-demo-karaf/etc/org.apache.karaf.features.cfg
sed -i '/mps-agent-feature.xml/d' /usr/share/cumulocity-demo-karaf/etc/org.apache.karaf.features.cfg

# start karaf service
service cumulocity-demo-karaf start
sleep 5
